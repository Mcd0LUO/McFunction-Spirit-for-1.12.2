    "tokenColors": [
        {
            "name": "MCFunction 行注释",
            "scope": "comment.line.mcfunction",
            "settings": {
                "foreground": "#6A9955",
                "fontStyle": "italic"
            }
        },
        {
            "name": "MCFunction 块注释（#>/#! 开头）",
            "scope": "comment.block.mcfunction",
            "settings": {
                "foreground": "#5A8A45", 
                "fontStyle": "italic"
            }
        },
        {
            "name": "MCFunction 注释内强调文本（#> 后的内容）",
            "scope": "markup.bold.mcfunction",
            "settings": {
                "foreground": "#A8D87C",
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "MCFunction 注释内@标记（如@rx97）",
            "scope": "markup.heading.mcfunction",
            "settings": {
                "foreground": "#87CEEB",
                "fontStyle": "bold"
            }
        },
        {
            "name": "MCFunction 普通命令（如execute、give）",
            "scope": "keyword.command.mcfunction",
            "settings": {
                "foreground": "#C9A0DC" 
            }
        },
        {
            "name": "MCFunction say命令（独立高亮，区分普通命令）",
            "scope": "keyword.control.flow.mcfunction",
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "name": "MCFunction NBT双引号字符串",
            "scope": "string.quoted.double.nbt.mcfunction",
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "MCFunction 普通双引号字符串",
            "scope": "string.quoted.double.mcfunction",
            "settings": {
                "foreground": "#FFA07A" 
            }
        },
        {
            "name": "MCFunction 单引号字符串",
            "scope": "string.quoted.single.mcfunction",
            "settings": {
                "foreground": "#FF8C69" 
            }
        },
        {
            "name": "MCFunction 转义字符（如\\n、\\\"）",
            "scope": "constant.character.escape.mcfunction",
            "settings": {
                "foreground": "#FFD700", 
                "fontStyle": "bold"
            }
        },
        {
            "name": "MCFunction 目标选择器（如@a、@p）",
            "scope": "support.class.mcfunction",
            "settings": {
                "foreground": "#9CDCFE" 
            }
        },
        {
            "name": "MCFunction 选择器参数（如tag=player、r=5）",
            "scope": "variable.parameter.selector.mcfunction",
            "settings": {
                "foreground": "#87CEEB" 
            }
        },
        {
            "name": "MCFunction 选择器分隔符（逗号）",
            "scope": "punctuation.separator.mcfunction",
            "settings": {
                "foreground": "#A9A9A9" 
            }
        },
        {
            "name": "MCFunction NBT结构（外层{}）",
            "scope": "meta.nbt.mcfunction",
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "MCFunction NBT键名（如CustomName:）",
            "scope": "variable.nbt.key.mcfunction",
            "settings": {
                "foreground": "#76EEC6"
            }
        },
        {
            "name": "MCFunction NBT数值（如123、45L）",
            "scope": "constant.numeric.nbt.mcfunction",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "MCFunction NBT分隔符（逗号）",
            "scope": "punctuation.separator.nbt.mcfunction",
            "settings": {
                "foreground": "#A9A9A9"
            }
        },
        {
            "name": "MCFunction 整数（如-5、100L）",
            "scope": "constant.numeric.integer.mcfunction",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "MCFunction 浮点数（如3.14、-0.5）",
            "scope": "constant.numeric.float.mcfunction",
            "settings": {
                "foreground": "#90EE90"
            }
        },
        {
            "name": "MCFunction 布尔值（true/false）",
            "scope": "constant.boolean.mcfunction",
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "MCFunction UUID（如123e4567-e89b-12d3-a456-426614174000）",
            "scope": "variable.uuid.mcfunction",
            "settings": {
                "foreground": "#DDA0DD"
            }
        },
        {
            "name": "MCFunction 资源名（如minecraft:zombie、pack/func）",
            "scope": "entity.name.function.mcfunction",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "MCFunction 坐标符号（~、^）",
            "scope": "constant.numeric.mcfunction",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "MCFunction 运算符（如+、=、:、,）",
            "scope": "keyword.operator.mcfunction",
            "settings": {
                "foreground": "#F0E68C" 
            }
        },
        {
            "name": "MCFunction Property键（如key:、#key）",
            "scope": "variable.other.mcfunction",
            "settings": {
                "foreground": "#87CEFA" 
            }
        }
    ]
    
}