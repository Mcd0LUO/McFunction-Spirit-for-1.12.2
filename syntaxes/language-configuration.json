{
    "comments": {
        "lineComment": {
            "comment": "#",
            "noIndent": false
        }
    },
    "brackets": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string"
            ]
        },
        {
            "open": "'",
            "close": "'",
            "notIn": [
                "string"
            ]
        }
    ],
    "surroundingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "'",
            "close": "'"
        }
    ],
    "indentationRules": {
        "increaseIndentPattern": "^.*\\{[^}]*$",
        "decreaseIndentPattern": "^.*\\}$"
    }
}